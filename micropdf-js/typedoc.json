{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "../docs/api/nodejs",
  "plugin": [],
  "name": "MicroPDF for Node.js",
  "readme": "./README.md",
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/test/**",
    "**/tests/**",
    "**/easy.ts",
    "**/simple.ts",
    "**/stext.ts",
    "node_modules/**"
  ],
  "skipErrorChecking": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "includeVersion": true,
  "sort": ["kind", "instance-first", "alphabetical"],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Document Operations",
    "Page Operations",
    "Rendering",
    "Text Extraction",
    "Geometry",
    "Buffer and Stream",
    "Colorspace",
    "Easy API",
    "Simple API",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "sourceLinkTemplate": "https://github.com/lexmata/micropdf/blob/{gitRevision}/{path}#L{line}",
  "gitRevision": "main",
  "githubPages": true,
  "hideGenerator": false,
  "customCss": "./typedoc-theme.css"
}
